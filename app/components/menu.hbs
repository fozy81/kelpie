


{{#if this.show}}
<Menu::List @id={{@id}} @label={{@label}}
 @edit={{@edit}} @add={{@add}}
  @model={{@model}} @archive={{@archive}} 
  @showArchive={{@showArchive}} 
  @showPrint={{@showPrint}}/>
{{else}}
<button  type="button"
class="pr-1 pl-1 bg-gray-200 rounded-md
 border-gray-400 border-gray-400 border-2
  shadow-sm" 
{{on "click" this.showCard}}>
<h3 class="text-sm">{{@title}}</h3>     
</button>

{{/if}}

